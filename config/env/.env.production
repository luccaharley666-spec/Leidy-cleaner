# .env.production
# Configurações de produção

NODE_ENV=production
PORT=3001
DEBUG=false

# Database
DATABASE_URL=${DB_URL_PRODUCTION}

# JWT
JWT_SECRET=${[REDACTED_TOKEN]}
JWT_EXPIRES_IN=30d

# Frontend
NEXT_PUBLIC_API_URL=https://api.limpezapro.com

# Payment Gateways
STRIPE_SECRET_KEY=${[REDACTED_TOKEN]}
MERCADOPAGO_TOKEN=${[REDACTED_TOKEN]}

# Email
SMTP_HOST=${[REDACTED_TOKEN]}
SMTP_PORT=587
SMTP_USER=${[REDACTED_TOKEN]}
SMTP_PASSWORD=${[REDACTED_TOKEN]}

# SMS/WhatsApp
TWILIO_SID=${[REDACTED_TOKEN]}
TWILIO_TOKEN=${[REDACTED_TOKEN]}

# Google Maps
GOOGLE_MAPS_API_KEY=${[REDACTED_TOKEN]}

# Redis
REDIS_URL=${[REDACTED_TOKEN]}

# Logging
LOG_LEVEL=warn
SENTRY_DSN=${[REDACTED_TOKEN]}
